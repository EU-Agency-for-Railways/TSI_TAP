			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="en">Visual signs in the coach</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation xml:lang="en">SOS button in toilet</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">SOS button near wheelchair seat</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation xml:lang="en">Ground guidelines for blinds</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation xml:lang="en">Low floor vehicle</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation xml:lang="en">PRM toilets</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">Speakers</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation xml:lang="en">Mobile ramp platform/coach</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="11">
				<xs:annotation>
					<xs:documentation xml:lang="en">Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PRMReplyCodeCodeList">
		<xs:annotation>
			<xs:documentation xml:lang="en">Code list for PRM reply codes (B.10.7)</xs:documentation>
			<xs:appinfo>
				<dc:title>Code list for PRM reservation reques reply codes</dc:title>
				<dc:source>B.10.7</dc:source>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:totalDigits value="3"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">Assistance available and confirmed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation xml:lang="en">a. not possible at this station for the requested train</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3">
				<xs:annotation>
					<xs:documentation xml:lang="en">a. not possible due to minimum change time</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="4">
				<xs:annotation>
					<xs:documentation xml:lang="en">a. not possible at this station at the required platform</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">a. not possible during the requested time</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="6">
				<xs:annotation>
					<xs:documentation xml:lang="en">wheelchair too heavy</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="7">
				<xs:annotation>
					<xs:documentation xml:lang="en">train data invalid</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="8">
				<xs:annotation>
					<xs:documentation xml:lang="en">station not found</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">a. to be arranged manually only</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation xml:lang="en">Too late for assistance booking</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PlatformSectionType">
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="PlatformType">
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="ServiceCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Leaflet 918 element 308 first two digit</xs:documentation>
			<xs:documentation xml:lang="en">
				A - 1st class seat
				B - 2nd class seat
				S - Single with shower and  WC
				T - Double with shower and  WC
				L - Single
				M - Special
				N - Double
				P - T2
				Q - T3
				R - T4
				C - Couchette 1st class
				D - Couchette 2nd Class
				D4 - Couchette 2nd Class four-berth
				D6 - Couchette 2nd Class six-berth
				E - Sleeperette 1st Class
				F - Sleeperette 2nd Class
				G - Premium 1st Class
				H - Premium 2nd Class
				K - Euraffaires
				O - Vehicle parking place
				O3 - Vehicle loading place category 1 - 3
				O8 - Vehicle loading place category 6 - 8
				SL - Single with shower and  WC and Double bed
				TL - Double with shower and WC and Double bed 
				U - T3 with shower and WC 
				X - Club 
				Y - Preferente 	
				Z - Tourist
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="T"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="N"/>
			<xs:enumeration value="P"/>
			<xs:enumeration value="Q"/>
			<xs:enumeration value="R"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="D"/>
			<xs:enumeration value="D4"/>
			<xs:enumeration value="D6"/>
			<xs:enumeration value="E"/>
			<xs:enumeration value="F"/>
			<xs:enumeration value="G"/>
			<xs:enumeration value="H"/>
			<xs:enumeration value="K"/>
			<xs:enumeration value="O"/>
			<xs:enumeration value="O3"/>
			<xs:enumeration value="O8"/>
			<xs:enumeration value="SL"/>
			<xs:enumeration value="TL"/>
			<xs:enumeration value="U"/>
			<xs:enumeration value="X"/>
			<xs:enumeration value="Y"/>
			<xs:enumeration value="Z"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StationFacilityType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Features provided at the station for PRMs</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int"/>
	</xs:simpleType>
	<xs:simpleType name="StationLocalCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">general local code for stations</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:maxInclusive value="99999"/>
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TrackType">
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="TrainServiceIdType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Train Id used for customer information. corresponds to TrainServiceNumber for numerical codes, but allows for alphanumerical codes (i.e. used by RZD)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:whiteSpace value="collapse"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="WeightUnitType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="g"/>
			<xs:enumeration value="kg"/>
		</xs:restriction>
	</xs:simpleType>
	<!--                   complex types                             -->
	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">Moved from passengercomplextypes.xsd</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Country" type="CountryCodeISO2AType"/>
			<xs:element name="City" type="xs:string"/>
			<xs:element name="Street" type="xs:token"/>
			<xs:element name="PostalCode" type="xs:token"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BookingRequestId">
		<xs:annotation>
			<xs:documentation source="identifies a Booking Request" xml:lang="en"/>
		</xs:annotation>
		<xs:attribute name="System" type="AssistanceBookingSystemType" use="required"/>
		<xs:attribute name="BookingId" type="BookingIdType" use="required"/>
	</xs:complexType>
	<xs:complexType name="ConfirmMessageReceivedType"/>
	<xs:complexType name="CustomerType">
		<xs:annotation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">Moved from passengercomplextypes.xsd</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FirstName" type="xs:token"/>
			<xs:element name="LastName" type="xs:token"/>
			<xs:element name="Title" type="xs:token" minOccurs="0"/>
			<xs:element name="DateOfBirth" type="xs:date" minOccurs="0"/>
			<xs:element name="Phone" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:token">
							<xs:attribute name="Preferred" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="MobilePhone" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:token">
							<xs:attribute name="Preferred" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="E-mail" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:token">
							<xs:attribute name="Preferred" type="xs:boolean" use="optional" default="false"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Language" type="LanguageType" minOccurs="0" maxOccurs="10"/>
			<xs:element name="Comment" type="xs:string" minOccurs="0"/>
			<xs:element name="Address" type="AddressType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DialogType">
		<xs:annotation>
			<xs:documentation source="identifies a dialog between the systems" xml:lang="en"/>
		</xs:annotation>
		<xs:attribute name="DialogId" type="DialogIdType"/>
		<xs:attribute name="Date" type="xs:date" use="required"/>
	</xs:complexType>
	<xs:complexType name="FailureReplyType">
		<xs:sequence>
			<xs:element name="AlternativeTrain" type="TrainServiceIdType" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">alternative trains suggested if the request for the original train could not be fullfilled</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NearestStation" type="StationCodeType" minOccurs="0" maxOccurs="5"/>
		</xs:sequence>
		<xs:attribute name="AssistanceId" type="AssistenceIdType" use="optional"/>
		<xs:attribute name="ReplyCode" type="PRMReplyCodeCodeList" use="required"/>
	</xs:complexType>
	<xs:complexType name="HandicapCardType">
		<xs:sequence>
			<xs:element name="ValidFrom" type="xs:date"/>
			<xs:element name="ValitUntil" type="xs:date"/>
			<xs:element name="Capacity" minOccurs="0">
				<xs:annotation>
					<xs:documentation doc:docType="Remark"> No data type. Probably Percentage of handicap see B.10 chapter 6.7</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LocationSubsidiaryType">
		<xs:attribute name="LocationSubsidiaryTypeCode">
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:enumeration value="00"/>
					<xs:enumeration value="01"/>
					<xs:enumeration value="02"/>
					<xs:enumeration value="03"/>
					<xs:enumeration value="04"/>
					<xs:enumeration value="05"/>
					<xs:enumeration value="06"/>
					<xs:enumeration value="07"/>
					<xs:enumeration value="08"/>
					<xs:enumeration value="09"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<!--		<xs:attribute ref="taf:LocationSubsidiaryTypeCode"/>-->
	</xs:complexType>
	<xs:complexType name="PrmCustomerType">
		<xs:complexContent>
			<xs:extension base="CustomerType">
				<xs:sequence>
					<xs:element name="Tools" type="PRMAssistanceFacilitiesCodeList" minOccurs="0" maxOccurs="3"/>
					<xs:element name="Disability" type="DisabilityTypeCodeList" maxOccurs="5"/>
					<xs:element name="HandicapCard" type="HandicapCardType" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="DisabilityDegree" type="DisabilityDegreeType" use="optional"/>
				<xs:attribute name="WheelChairChange" type="xs:boolean" use="optional"/>
				<xs:attribute name="PriorityCard" type="xs:boolean" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PrmStationDescrType">
		<xs:annotation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">Moved from passengercomplextypes.xsd</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Station" type="ServiceCodeType"/>
			<xs:element name="ChangeTime" minOccurs="0" maxOccurs="4">
				<xs:annotation>
					<xs:documentation xml:lang="en">minimal time required to change trains</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:nonNegativeInteger">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="60"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Facility" type="StationFacilityType" minOccurs="0" maxOccurs="20"/>
			<xs:element name="Platform" minOccurs="0" maxOccurs="30">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Facility" type="StationFacilityType" minOccurs="0" maxOccurs="20"/>
					</xs:sequence>
					<xs:attribute name="PlatformNb" type="PlatformType" use="required"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PrmAccessable" type="xs:boolean" use="required"/>
	</xs:complexType>
	<xs:complexType name="PrmTrainDescrType">
		<xs:annotation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">Moved from passengercomplextypes.xsd</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Departure" type="TrainStopType"/>
			<xs:element name="Facility" type="PRMOnBoardFacilitiesCodeList" minOccurs="0" maxOccurs="20"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RequestorType">
		<xs:annotation>
			<xs:documentation source="identifies a requesting system" xml:lang="en"/>
		</xs:annotation>
		<xs:attribute name="System" type="AssistanceBookingSystemType" use="required"/>
		<xs:attribute name="Company" type="CompanyCodeType" use="required"/>
	</xs:complexType>
	<xs:complexType name="StationAssistanceType">
		<xs:annotation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">Moved from passengercomplextypes.xsd</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice maxOccurs="2">
				<xs:element name="ArrivalTrain" type="TrainStopType">
					<xs:annotation>
						<xs:documentation xml:lang="en">plattform is optional and not given in requests. If given in the reply it has to be checked by the RU for corectness. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DepartureTrain" type="TrainStopType">
					<xs:annotation>
						<xs:documentation xml:lang="en">plattform is optional and not given in requests. If given in the reply it has to be checked by the RU for corectness. </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="RequiredTool" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="Type" type="PRMAssistanceFacilitiesCodeList" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="AccompaningPerson" type="CustomerType" minOccurs="0"/>
			<xs:element name="EmergencyContact" type="CustomerType" minOccurs="0"/>
			<xs:element name="CustomerProvidedTool" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="Type" type="PRMAssistanceFacilitiesCodeList" use="optional"/>
					<xs:attribute name="Weight" type="xs:int" use="optional"/>
					<xs:attribute name="WeigtUnit" type="WeightUnitType" use="optional" default="kg"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Comment" type="xs:string" minOccurs="0"/>
			<xs:element name="StationAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Station address is givenb to provide an address to the customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="BookingStatus" use="required">
			<xs:simpleType>
				<xs:restriction base="BookingStatusType"/>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="NbLuggage" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:int">
					<xs:minInclusive value="0"/>
					<xs:maxInclusive value="9"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="StationCodeType">
		<xs:annotation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">Moved from passengercomplextypes.xsd</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="CountryCode" type="CountryCodeType"/>
				<xs:element name="Country" type="CountryCodeType"/>
			</xs:choice>
			<xs:choice>
				<xs:element name="LocationPrimaryCode" type="StationLocalCodeType"/>
				<xs:element name="LocalCode" type="StationLocalCodeType"/>
			</xs:choice>
			<xs:element name="LocationSubsidiaryCode" type="LocationSubsidiaryType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Context" type="ContextType" default="UIC"/>
	</xs:complexType>
	<xs:complexType name="TrainStopType">
		<xs:annotation>
			<xs:documentation xml:lang="en">identification of a train stop</xs:documentation>
			<xs:documentation doc:docType="History" doc:Person="" doc:date="">moved from passengercomplextypes,xsd and extract codelist</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Station" type="StationCodeType"/>
			<xs:element name="Train">
				<xs:complexType>
					<xs:attribute name="TrainNb" type="TrainServiceIdType" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Platform" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:attribute name="Platform" type="PlatformType" use="optional"/>
					<xs:attribute name="Track" type="TrackType" use="optional"/>
					<xs:attribute name="Section" type="PlatformSectionType" use="optional"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Time" type="xs:dateTime" use="required"/>
	</xs:complexType>
</xs:schema>
