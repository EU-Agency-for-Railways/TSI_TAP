	<xs:simpleType name="AssistanceBookingSystemType">
		<xs:annotation>
			<xs:documentation xml:lang="en">System to allocate PRM assistance for a set of stations</xs:documentation>
			<xs:documentation doc:docType="Remark"> Is this number linked to a reservation system from B.5?</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AssistenceIdType">
		<xs:annotation>
			<xs:documentation xml:lang="en">unique identification of an assistance in a message, used to link fialure replies to assistance in multiple assistance requests</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:int">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BookingIdType">
		<xs:annotation>
			<xs:documentation xml:lang="en">idebntifies a booking within a system</xs:documentation>
			<xs:documentation doc:docType="Remark"> What is the difference to the BookiingRequestId?</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BookingStatusType">
		<xs:annotation>
			<xs:documentation doc:docType="Remark"> No definition of data type found</xs:documentation>
			<xs:documentation xml:lang="en">Booking status</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CompanyCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Identifies the RU, IM or other company involved in the Rail Transport Chain  (i.e. 1088 for passenger SNCB). The code list is published by UIC.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Z]{4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ContextType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The context type defines context values to distinguish different coding lists defined for the same items. Numbers refere to the UIC leaflets.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="TSI"/>
			<xs:enumeration value="918"/>
			<xs:enumeration value="920"/>
			<xs:enumeration value="108"/>
			<xs:enumeration value="UIC"/>
			<xs:enumeration value="ISO"/>
			<xs:enumeration value="301"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryCodeISO2AType">
		<xs:annotation>
			<xs:documentation xml:lang="en">ISO 3166-1 alpha code (2 positions)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="CountryCodeType">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">general country code type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DialogIdType">
		<xs:annotation>
			<xs:documentation xml:lang="en">identification for a dialogue</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="999999"/>
		</xs:restriction>
	</xs:simpleType>
