<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.era.europa.eu/schemes/TAPTSI/1.4" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:doc="http://www.era.europa.eu/schemes/Documentation" xmlns:taf="http://www.era.europa.eu/schemes/TAFTSI/5.3" xmlns:dc="http://purl.org/dc/elements/1.1/" targetNamespace="http://www.era.europa.eu/schemes/TAPTSI/1.4" elementFormDefault="unqualified" version="1.4">
	<xs:annotation>
		<xs:appinfo>
			<dcterms:title>TAP TSI - International Rail Ticket for Home Printing.</dcterms:title>
			<dcterms:description>This schema defines the International Rail Ticket for Home Printing.</dcterms:description>
			<dcterms:creator>TAP TSI Editor Stefan Jugelt, European Railway Agency. mailto:stefan.jugelt@era.europa.eu</dcterms:creator>
			<dcterms:coverage>European Union</dcterms:coverage>
			<dcterms:created>2011-05-05</dcterms:created>
			<dcterms:changed>2012-03-08</dcterms:changed>
			<dcterms:changed>2013-07-03</dcterms:changed>
			<dcterms:changed>2020-05-27</dcterms:changed>
			<dcterms:references>ERA/TD/2009-10/INT</dcterms:references>
			<dcterms:type>text/xml</dcterms:type>
			<dcterms:subject>Transport, Public Transport, Rail</dcterms:subject>
			<dcterms:identifier>{http://www.era.europa.eu/schemes/TAPTSI}TAP_TSI_B.7.xsd</dcterms:identifier>
			<dcterms:language>en</dcterms:language>
			<dcterms:publisher>ERA European Railway Agency, 120 rue Marc Lefrancq, 59307 Valenciennes Cedex, http://www.era.europa.eu/</dcterms:publisher>
			<dcterms:rights>Unclassified</dcterms:rights>
			<dcterms:rightsHolder>European Railway Agency</dcterms:rightsHolder>
			<dcterms:copyright>ERA 2020</dcterms:copyright>
			<dcterms:source>Derived from the UIC leaflet 918-3</dcterms:source>
		</xs:appinfo>
	</xs:annotation>
	<xs:element name="MessageHeader">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MessageHeaderType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Identification of the type of the message being interchanged.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="UPR"/>
							<xs:enumeration value="UPD"/>
							<xs:enumeration value="INR"/>
							<xs:enumeration value="IND"/>
							<xs:enumeration value="ACK"/>
							<xs:enumeration value="ORI"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="Version" type="xs:decimal">
					<xs:annotation>
						<xs:documentation xml:lang="en">Version number of a message type.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="From" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="en">Identifies the Party that orginated the message. Restricted to 4N for ENEE.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="To" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="en">Identifies the Party that is the intended recipient of the message. Restricted to 4N for ENEE.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MessageId" type="xs:nonNegativeInteger">
					<xs:annotation>
						<xs:documentation xml:lang="en">Unique identifier for the message.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Timestamp" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation xml:lang="en">Value representing the time that the message header was created conforming to a dateTime [CCYY-MM-DDTHH:MM:SS] and MUST be expressed as UTC. Indicating UTC in the Timestamp element by including the 'Z' identifier is optional. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RefToMessageId" type="xs:nonNegativeInteger" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="en">When present, it MUST contain the MessageId value of an earlier message to which this message relates. If there is no ealier related message, the element MUST NOT be present.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FreeText">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SequenceNumber">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="2"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Supplement" type="xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used in the HomeTicketing XML message</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PassengerType" type="type5261CodeList">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used in the HomeTicketing XML message</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CancellationInformation">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used in the HomeTicketing XML message</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CertificateRequestID" type="TCO_ReferenceType"/>
				<xs:element name="TCOTransactionID" type="TCO_ReferenceType"/>
				<xs:element name="CancellationReasonCode" type="CancellationReasonCodeList">
					<xs:annotation>
						<xs:documentation xml:lang="en">Code list for the reason code introduced. (Stefan Jugelt, ERA)</xs:documentation>
						<xs:documentation doc:docType="Remark">The item TCOXXXX must defined outside the code list!</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CancellationAcknowledgement">
		<xs:annotation>
			<xs:documentation xml:lang="en">Cancellation acknowledgement (CA)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MessageHeader"/>
				<xs:element name="CancellationReferenceID" type="RailwayUndertaking_ReferenceType"/>
				<xs:element name="AckError">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="200"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CancellationMessage">
		<xs:annotation>
			<xs:documentation xml:lang="en">Cancellation message (CM)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MessageHeader"/>
				<xs:element name="ReferenceID_Cancellation" type="RailwayUndertaking_ReferenceType"/>
				<xs:element name="RecordNumber">
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:minInclusive value="0"/>
							<xs:maxInclusive value="999999"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element ref="CancellationInformation" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CertificateRequest">
		<xs:annotation>
			<xs:documentation xml:lang="en">Certificate request (CR)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MessageHeader"/>
				<xs:element name="Distributor_Reference" type="RailwayUndertaking_ReferenceType"/>
				<xs:element name="TicketHolder" type="Person"/>
				<xs:element name="TicketCharacteristics" type="Ticket"/>
				<xs:element name="CertificateRequest_Details" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Passenger_Fields">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="SequenceNumber"/>
										<xs:element ref="PassengerType"/>
										<xs:element name="TariffReduction" type="TariffTypeDetails"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="OpenTicketsFields" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="SequenceNumber"/>
										<xs:element name="Segment" type="Segment" maxOccurs="unbounded"/>
										<xs:element ref="Supplement"/>
										<xs:element name="TariffCode" type="TariffTypeIdentification" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="TariffReduction" type="TariffTypeDetails" minOccurs="0"/>
										<xs:element name="Date" type="Period"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="ReservationFields" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="SequenceNumber"/>
										<xs:element name="FormerReference" type="Reference" minOccurs="0"/>
										<xs:element name="PassengerTripInformation" type="PassengerTripInformation"/>
										<xs:element name="Segment" type="Segment" maxOccurs="unbounded"/>
										<xs:element ref="Supplement"/>
										<xs:element name="TariffCode" type="TariffTypeIdentification" minOccurs="0">
											<xs:annotation>
												<xs:documentation xml:lang="en">According to TAP TSI technical document B.5 element 42</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="TariffReduction" type="TariffTypeDetails" minOccurs="0"/>
										<xs:element name="Date" type="Period"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CertificateTransfer">
		<xs:annotation>
			<xs:documentation xml:lang="en">Certificate transfer (CT)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MessageHeader"/>
				<xs:element name="Distributor_Reference" type="RailwayUndertaking_ReferenceType">
					<xs:annotation>
						<xs:documentation xml:lang="en">The Reference number of the certificate request.  This is concatenated with the identification of the distributor(same number as in the CR message).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TCO_Transaction" type="TCO_ReferenceType"/>
				<xs:element name="TripInformation" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="SequenceNumber"/>
							<xs:element name="Segment" type="Segment"/>
							<xs:element name="TCOCode" type="ReferenceNumber"/>
							<xs:element name="ClientIdentification" type="Person"/>
							<xs:element name="CertificatePresentationCode" type="CertificatePresentationCodeList"/>
							<xs:element name="Certificate">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="0"/>
										<xs:maxLength value="200"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:sequence>
					<xs:element name="Error" type="ErrorCode_HomePrintingCodeList"/>
					<xs:element ref="FreeText" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="MainRecordDataType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Data type U_HEAD</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RecordID" type="RecordIdType" default="U_HEAD">
				<xs:annotation>
					<xs:documentation xml:lang="en">ID is "U_HEAD" ("header")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecordVersion" type="RecordVersionType" default="01">
				<xs:annotation>
					<xs:documentation xml:lang="en">"01"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecordLength" type="RecordLengthType"/>
			<xs:element name="CompanyCodeOfDistributor" type="CompanyCodeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Distributor, producing the tickets</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnambiguousKeyOfTickets" type="KeyOfTicketsType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The producer can complete this key freely; a specific key can only be used for one ticket. In combination with the Company code this key provides a global unique key for each ticket within the EU.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EditionTime" type="EditionTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Format "DDMMYYYYHHMM"
This can be used to prevent fraud by buying a ticket after journey departure (only shortly before the conductor shows up)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Flags">
				<xs:annotation>
					<xs:documentation xml:lang="en">- International ticket: 1
- Edited by agent: 2
- Specimen: 4
The field is the decimal representation of the sum according to the flags set. E.g.: "6" means national ticket, edited by agent, specimen</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="1"/>
						<xs:maxInclusive value="7"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="EditionLanguageOfTicket" type="LanguageType">
				<xs:annotation>
					<xs:documentation xml:lang="en">ISO 639 - 1 (see Bibliography - page 78) country code definitions and abbreviations</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EditionLanguageCodeOfCarriage" type="LanguageType">
				<xs:annotation>
					<xs:documentation xml:lang="en">ISO 639 - 1 country code. If no second language is used, this field is filled with blanks</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TicketLayout">
		<xs:annotation>
			<xs:documentation xml:lang="en">Data type U_TLAY</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RecordID" type="RecordIdType" default="U_TLAY">
				<xs:annotation>
					<xs:documentation xml:lang="en">ID is "U_TLAY"("Ticket layout")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecordVersion" type="RecordVersionType" default="01">
				<xs:annotation>
					<xs:documentation xml:lang="en">"01"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RecordLength" type="RecordLengthType"/>
			<xs:element name="LayoutStandard">
				<xs:annotation>
					<xs:documentation xml:lang="en">For example "RCT2"</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NumberOfFields" type="NumberOfFieldsType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Number of following fields</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:sequence maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en" doc:docType="Remark">There should be a maximum number of entries</xs:documentation>
				</xs:annotation>
				<xs:element name="FieldLine" type="FieldLineType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Line index of the first letter. Range: 0 till 14</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FieldColumn" type="FieldColumnType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Column index of the first letter. Range: 0 till 71</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FieldHeight" type="FieldHeightType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Number of lines reserved from the field</xs:documentation>
						<xs:documentation doc:docTpe="Remark">The boundaries for this type shall be defined</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FieldWidth" type="FieldWidthType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Number of columns reserved from the field</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FieldFormatting" type="FieldFormattingTypeCodeList">
					<xs:annotation>
						<xs:documentation xml:lang="en">See code list B.7.6</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FieldTextLength" type="FieldLengthType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Length of the following text. Caution: the length must be determined using the already encoded text</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="FieldText" type="xs:string">
					<xs:annotation>
						<xs:documentation xml:lang="en">If "Field height" is bigger than 1, the text must be wrapped using the following rules:
-	The first word that doesn't fit in the current field line, is printed at the beginning of the following line
-	If the field text contains the sign LF (ASCII "10"), the next word must be wrapped to the next line
The producer of the record must guarantee that the entire text, when applying these rules, fits within the indicated field sizes (height and width), can be entirely displayed
</xs:documentation>
						<xs:documentation xml:lang="en" doc:docType="Remark">There should be a length restriction for this field !</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DSTMessageType">
		<xs:annotation>
			<xs:documentation xml:lang="en">DST message structure</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UniqueMessageId">
				<xs:annotation>
					<xs:documentation xml:lang="en">"#UT" for TAP TSI Ticket</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="MessageTypeVersion" type="RecordVersionType">
				<xs:annotation>
					<xs:documentation xml:lang="en">"01" (Version of this Technical Document)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SigningRailwayUndertaking" type="CompanyCodeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Allows the identification of the public key necessary for the verification in combination with the following element</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SignatureKeyID" type="SignatureIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The ID must be managed by the RU issuing the signature key</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Signature" type="SignatureType">
				<xs:annotation>
					<xs:documentation xml:lang="en">DSA signature for the message after compression (next element) in ASN.1 representation (see list of abbreviations). If the actual ASN.1 representation is shorter than 50 bytes, it should be enlarged to the length of 50 by adding null bytes</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LengthOfCompressedMessage" type="MessageLengthType"/>
			<xs:element name="CompressedMessage" type="xs:string">
				<xs:annotation>
					<xs:documentation xml:lang="en">The entire record sequence compressed with DEFLATE</xs:documentation>
					<xs:documentation xml:lang="en" doc:docType="Remark">Boundaries are not defined</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType><!--                   +++++++++++                            --><!--                   type defintions                             --><!--                   +++++++++++                            --><!--                   attribute                                    --><!--                   simple types                             --><xs:simpleType name="CancellationReasonCodeList">
		<xs:annotation>
			<xs:documentation xml:lang="en">Code list for the cancellation (B.7.3)</xs:documentation>
			<xs:appinfo>
				<dc:title>Code list for the cancellation</dc:title>
				<dc:source>B.7.3</dc:source>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="7"/>
			<xs:enumeration value="TO">
				<xs:annotation>
					<xs:documentation xml:lang="en">Time out</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NA">
				<xs:annotation>
					<xs:documentation xml:lang="en">No answer from TCO</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NAOXXXX">
				<xs:annotation>
					<xs:documentation xml:lang="en">No Answer from Other TCO, XXXX = Company code of TCO</xs:documentation>
					<xs:documentation xml:lang="en" doc:docType="Remark">XXXX = Company code of TCO</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TAC">
				<xs:annotation>
					<xs:documentation xml:lang="en">Annulation of ticket by client</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TAD">
				<xs:annotation>
					<xs:documentation xml:lang="en">Annulation of ticket by distributor</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PI">
				<xs:annotation>
					<xs:documentation xml:lang="en">Procedure interrupted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CertificatePresentationCodeList">
		<xs:annotation>
			<xs:documentation xml:lang="en">code list for the presentation type of the certificate (B.7.2)</xs:documentation>
			<xs:appinfo>
				<dc:title>Code list for the layout of the certificate</dc:title>
				<dc:source>B.7.2</dc:source>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:enumeration value="TEXT">
				<xs:annotation>
					<xs:documentation xml:lang="en">ASCII-code</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3OF9">
				<xs:annotation>
					<xs:documentation xml:lang="en">3 of 9 barcode</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AZTXYZ">
				<xs:annotation>
					<xs:documentation xml:lang="en">2D-barcode type AZTEC XYZ</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHIP">
				<xs:annotation>
					<xs:documentation xml:lang="en">Chipcard</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CharacteristicDescriptionCode">
		<xs:annotation>
			<xs:documentation xml:lang="en">Based on UIC Leaflet 920-16, Tag 7037 for describing the Class of Service.  </xs:documentation>
			<xs:documentation doc:docType="Remark"> What is the differnce to the type7037CodeList?</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="6">
				<xs:annotation>
